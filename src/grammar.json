{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "emmet",
  "rules": {
    "snippet": {
      "type": "SYMBOL",
      "name": "_node"
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "SYMBOL",
          "name": "siblings"
        },
        {
          "type": "SYMBOL",
          "name": "children"
        },
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "multiplication"
        }
      ]
    },
    "multiplication": {
      "type": "PREC_RIGHT",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_node"
          },
          {
            "type": "STRING",
            "value": "*"
          },
          {
            "type": "PATTERN",
            "value": "\\d+"
          }
        ]
      }
    },
    "children": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_node"
          },
          {
            "type": "STRING",
            "value": ">"
          },
          {
            "type": "SYMBOL",
            "name": "_node"
          }
        ]
      }
    },
    "siblings": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_node"
          },
          {
            "type": "STRING",
            "value": "+"
          },
          {
            "type": "SYMBOL",
            "name": "_node"
          }
        ]
      }
    },
    "group": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_node"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_identifier": {
      "type": "PATTERN",
      "value": "\\p{XID_Start}[\\p{XID_Continue}$-]*",
      "flags": "u"
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "name"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "name"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "id"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "class"
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "text"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "property"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "name": {
      "type": "SYMBOL",
      "name": "_identifier"
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_identifier"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "class": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_identifier"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "id": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_identifier"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "text": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "PATTERN",
          "value": "[^}]+"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}